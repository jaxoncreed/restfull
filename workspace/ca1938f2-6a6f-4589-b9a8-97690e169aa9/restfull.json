{
    "repoName": "Music",
    "repoDescription": "A music repo",
    "framework": "sails",
    "database": "mongo",
    "schemas": [
        {
            "name": "Song",
            "attributes": [
                {
                    "key": "title",
                    "type": "string",
                    "autoIncrement": false,
                    "unique": true,
                    "primaryKey": false,
                    "required": true,
                    "collection": false
                },
                {
                    "key": "duration",
                    "type": "float",
                    "autoIncrement": false,
                    "unique": false,
                    "primaryKey": false,
                    "required": false,
                    "collection": false
                }
            ]
        },
        {
            "name": "Album",
            "attributes": [
                {
                    "key": "title",
                    "type": "string",
                    "autoIncrement": false,
                    "unique": true,
                    "primaryKey": false,
                    "required": true,
                    "collection": false
                },
                {
                    "key": "songs",
                    "autoIncrement": false,
                    "unique": false,
                    "primaryKey": false,
                    "required": false,
                    "collection": "Song",
                    "via": "Album"
                }
            ]
        }
    ]
}